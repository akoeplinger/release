username=builder
hostname=monobuild2.boston.ximian.com
root_dir=/jails/suse-103-x86_64
target_command_prefix=". /etc/profile;"
# suse 10.2 doesn't include /usr/lib/pkgconfig on x86_64, force it.
env="PKG_CONFIG_PATH=$PKG_CONFIG_PATH:/usr/lib/pkgconfig"

chroot_path=/usr/bin/chroot

# NOTE: We edited /usr/lib/libmoon.la to replace /opt/gnome with /usr

# Repo conf
YAST_INSTALL=1
RPM_MD_REPO=1
